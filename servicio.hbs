{{!--
This template is used for the comunity page.
--}}

{{!-- The tag below means: insert everything in this file
into the {body} of the default.hbs template --}}
{{!< default}}

{{> tag_page categoria="servicio"}}
  <main>
    <div class="l-content">
      <div class="l-wrapper" data-aos="fade-up" data-aos-delay="300">
        <div class="l-grid centered">
          {{!-- The tag below iterates over all the pages --}}
          {{#get "pages" filter="tags:servicio" include="tags,authors"}}
            {{> "pageloop"}}
          {{/get}}
        </div>
      </div>
    </div>
  </main>
</div>